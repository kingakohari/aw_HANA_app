{
  "name": "MyHANAApp",
  "version": "1.0.0",
  "description": "A simple CAP project.",
  "repository": "https://github.com/kingakohari/aw_HANA_app.git",
  "license": "UNLICENSED",
  "private": true,
  "dependencies": {
    "@sap/cds": "^6",
    "express": "^4",
    "hdb": "^0.19.0"
  },

  "scripts": {
    "start": "cds run"
  },
  "cds": {
    "build": {
        "tasks": [
            {
                "for": "hana",
                "dest": "../db"
            },
            {
                "for": "node-cf"
            }
        ]
    },
    "requires": {
        "db": {
            "kind": "hana-cloud"
        }
    }
}
}
